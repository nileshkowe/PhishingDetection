{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        },
        {
            "src": "prediction.py",
            "use": "@vercel/python"
        },
        {
            "src": "preprocess.py",
            "use": "@vercel/python"
        },
        {
          "functions": {
            "api": {
              "runtime": "python3.9"
            }
          }
        }

    ],
    
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.py"
        }
    ]
      
}
