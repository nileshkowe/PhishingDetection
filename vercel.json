{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        },
        {
            "src": "prediction.py",
            "use": "@vercel/python"
        },
        {
            "src": "preprocess.py",
            "use": "@vercel/python"
        } 
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.py"
        }
    ]
      
}